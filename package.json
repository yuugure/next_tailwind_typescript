    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "10.5.2",
    "postcss": "^8.1.9",
    "postcss-preset-env": "^6.7.0",
    "precommit": "^1.2.2",
    "prettier": "^2.2.0",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.1.2"
